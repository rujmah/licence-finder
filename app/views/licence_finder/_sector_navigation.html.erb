<ul class="sector-navigation">
    <% @sectors.each do |sector| %>
       <li>
           <% if from == "sector_index" %>
               <%= link_to sector.name, browse_sector_path(:sector => sector.public_id) %>
           <% elsif from == "sector" %>
               <%= link_to sector.name, browse_sector_child_path(:sector_parent => @sector.public_id, :sector => sector.public_id) %>
           <% else %>
               <%= sector.name %>
           <% end %>
       </li>
    <% end %>
</ul>
